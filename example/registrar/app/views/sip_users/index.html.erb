<h1>Listing sip_users</h1>

<table>
  <tr>
    <th>User name</th>
    <th>First name</th>
    <th>Last name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sip_users.each do |sip_user| %>
  <tr>
    <td><%= sip_user.user_name %></td>
    <td><%= sip_user.first_name %></td>
    <td><%= sip_user.last_name %></td>
    <td><%= link_to 'Show', sip_user %></td>
    <td><%= link_to 'Edit', edit_sip_user_path(sip_user) %></td>
    <td><%= link_to 'Destroy', sip_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

